    <description>Oppdaterer felt brukt til å måle datakvalitet.</description>
    <environments>Default</environments>
    <formulas>
        <name>firstActivityDate</name>
        <dataType>Date</dataType>
        <expression>IF( ISBLANK({!taskDate}), {!eventDate},
  IF( ISBLANK({!eventDate}), {!taskDate},
    IF({!taskDate} &lt;{!eventDate}, {!taskDate} ,{!eventDate} )
  )
)</expression>
    </formulas>
    <formulas>
        <description>Feilmelding må ikke overskride størrelsen på feltet</description>
        <name>FlowFaultMessage</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Flow.FaultMessage}, 32000)</expression>
    </formulas>
    <formulas>
        <name>hasAdvancedInputParameters</name>
        <dataType>Boolean</dataType>
        <expression>{!hasBasicInputParameters} &amp;&amp; OR(  NOT(ISBLANK({!activityType})),  NOT(ISBLANK({!ownerId})) )</expression>
    </formulas>
    <formulas>
        <name>hasBasicInputParameters</name>
        <dataType>Boolean</dataType>
        <expression>NOT(OR(ISBLANK({!accountId}), ISBLANK({!recordId}), ISBLANK({!referenceDate}), ISBLANK({!daysBefore}), ISBLANK({!daysAfter})))</expression>
    </formulas>
    <formulas>
        <name>searchEndDate</name>
        <dataType>Date</dataType>
        <expression>{!referenceDate} + {!daysAfter}</expression>
    </formulas>
    <formulas>
        <name>searchStartDate</name>
        <dataType>Date</dataType>
        <expression>{!referenceDate} - {!daysBefore}</expression>
    </formulas>
