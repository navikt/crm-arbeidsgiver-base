        <formula>IF( ISBLANK({!ownerId}), TRUE, {!currentItem_Filter_events.OwnerId} == {!ownerId} ) &amp;&amp; IF( ISBLANK({!activityType}), TRUE, CONTAINS( {!activityType}, TEXT({!currentItem_Filter_events.TAG_ActivityType__c})) )</formula>
