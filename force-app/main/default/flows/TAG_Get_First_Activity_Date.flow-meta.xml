        <elementSubtype>FilterCollectionProcessor</elementSubtype>
        <label>Filter events</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignNextValueToReference>currentItem_Filter_events</assignNextValueToReference>
        <collectionProcessorType>FilterCollectionProcessor</collectionProcessorType>
        <collectionReference>Find_related_events</collectionReference>
        <conditionLogic>formula_evaluates_to_true</conditionLogic>
        <connector>
