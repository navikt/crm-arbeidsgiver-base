<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TAG_Is_My_NAV_Market_Region__c</fullName>
    <description>Display true if users market region matches market region on accounts related Nav unit.
Used as filter.</description>
    <externalId>false</externalId>
    <formula>AND(
LOWER($User.CRM_MarketRegion__c) = LOWER(TAG_NavUnit__r.MarketRegion__c),
RecordType.DeveloperName = &apos;Employer&apos;
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is My NAV Market Region</label>
    <type>Checkbox</type>
</CustomField>
