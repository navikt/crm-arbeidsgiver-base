<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TAG_CompletedLateNrOfHours__c</fullName>
    <description>Shows the number of hours that has passed since Due Date of the task, until the task is actual set to completed</description>
    <externalId>false</externalId>
    <formula>IF((TAG_CompletedDate__c - DATETIMEVALUE(ActivityDate)) * 24 &gt; 24, 
			ROUND(((TAG_CompletedDate__c - DATETIMEVALUE(ActivityDate)) * 24) - 24, 0), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Completed Late Nr of Hours</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
